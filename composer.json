{
  "name": "mohsinadeel/wpcoreintegrity",
  "description": "A plugin to scan WordPress core directories to check the files integrity",
  "type": "project",
  "require": {
    "php": "^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Mohsin Adeel",
      "email": "mohsin.adeel@yahoo.com"
    }
  ],
  "config": {
    "bin-dir": "vendor/bin"
  },
  "autoload": {
    "psr-4": {
      "mohsinadeel\\wpcoreintegrity\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/"
    }
  }
}
